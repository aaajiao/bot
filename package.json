{
  "name": "crazyplayer",
  "version": "1.0.0",
  "description": "A Player for Dom Elements",
  "author": "Elvis",
  "private": true,
  "scripts": {
    "pretest": "mongod --dbpath data --fork --logpath /dev/null",
    "test": "backpack dev",
    "dev": "nuxt",
    "build": "nuxt build && backpack build",
    "prestart": "mongod --dbpath data --fork --logpath /dev/null",
    "start": "cross-env NODE_ENV=production node build/main.js",
    "stop": "mongo admin --eval 'db.shutdownServer()' > /dev/null",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.18.2",
    "bulma": "^0.5.3",
    "cross-env": "^5.0.5",
    "del": "^3.0.0",
    "express": "^4.16.2",
    "express-fileupload": "^0.3.0",
    "font-awesome": "^4.7.0",
    "form-data": "^2.3.1",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "mongoose": "^4.12.1",
    "mongoose-lean-virtuals": "^0.1.1",
    "nuxt": "^1.0.0-rc11",
    "serve-favicon": "^2.4.5",
    "vue-paginate": "^3.5.1",
    "vue-upload-component": "^2.6.0-beta.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "backpack-core": "^0.4.3",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6"
  }
}
